{
  "kind": "collectionType",
  "collectionName": "gallery-object",
  "info": {
    "name": "GalleryObject",
    "description": "Represents a gallery object with properties related to title, user, date, likes, views, and discussion board."
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": false
  },
  "attributes": {
    "title": {
      "type": "string"
    },
    "user_name": {
      "type": "string"
    },
    "date_posted": {
      "type": "datetime"
    },
    "like_count": {
      "type": "integer"
    },
    "view_count": {
      "type": "integer"
    },
    "discussion_board": {
      "type": "json"
    },
    "objectId": {
      "type": "integer",
      "required": true
    },
    "type": {
      "type": "enumeration",
      "enum": ["project", "block", "lesson"]
    },
    "visibility": {
      "type": "enumeration",
      "enum": ["organization", "classroom", "public"]
    }
  }
}
